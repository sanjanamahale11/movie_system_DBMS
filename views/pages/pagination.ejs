<!DOCTYPE html>
<html>
<head>
    <title></title>
    <link rel="stylesheet" type="text/css" href="/pagination/page.css">
    <script type="text/javascript" src="/jquery.js"></script>
    <script type="text/javascript" src="/pagination/page.js"></script>
</head>
<body>
<div class="pagination-wrapper">
  <div class="pagination">
    <a class="prev page-numbers" href="/page/<%=type2%>/<%=pno-1%>">prev</a>
    <% 
    if(totalpg > 7)
    totalpg=7
    for(i=1;i<=totalpg;i++){
         if(i==pno && i==1){
         %>
              <a class="page-numbers current first" href="/page/<%=type2%>/<%=i%>"><%=i%></a>
         <%
       }

        else if(i==pno){
         %>
              <a class="page-numbers current" href="/page/<%=type2%>/<%=i%>"><%=i%></a>
         <%
       }

       else{
       %>
             <a class="page-numbers " href="/page/<%=type2%>/<%=i%>"><%=i%></a>

       <%
     }
  }
  %>
   <a class="next page-numbers" href="/page/<%=type2%>/<%=Number(pno)+1%>">next</a>
  </div>
</div>
</body>
</html>
